<title>BoxPlot And Scatter Chart</title>

<chart>
<options>
chart: {
  type: 'boxPlot',
  height: 350
},
title: {
  text: 'BoxPlot - Scatter Chart',
  align: 'left'
},
xaxis: {
  type: 'datetime',
  tooltip: {
    formatter: function(val) {
      return new Date(val).getFullYear()
    }
  }
},
fill: {
  colors: ['#FEB019']
},
tooltip: {
  shared: false,
  intersect: true
}
</options>

<series>
[
  {
    name: 'box',
